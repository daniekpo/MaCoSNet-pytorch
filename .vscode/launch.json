    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Eval",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/eval_qualitative.py",
            "args": [
                "--model", "trained_models/model.pth.tar"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Train",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--training-dataset",
                "butterfly",
                "--num-epochs",
                "60",
                "--lr",
                "5e-8",
                "--gpu",
                "0",
                "--num-workers",
                "4",
                "--batch-size",
                "4",
                "--result-model-dir",
                "trained_models",
                "--match-loss",
                "True",
                "--cycle-loss",
                "True",
                "--trans-loss",
                "False",
                "--coseg-loss",
                "False",
                "--task-loss",
                "False",
                "--w-match",
                "1.0",
                "--w-cycle",
                "1.0",
                "--w-trans",
                "0.0",
                "--w-coseg",
                "0.0",
                "--w-task",
                "0.0",
            ],
            "console": "integratedTerminal"
        }
    ]
}